<% if @error.present? %>
  showError('<%= j @error %>');
<% else %>
  unbanMembership(<%= j @service.membership.id.to_s %>);
  showSuccess('<%= j t('admin2.manage_users.saved') %>');
  processDeleteUser('<%= @service.membership.id %>', <%= @can_delete %>, '<%= j @delete_title %>');
  var current = $('#status_').val();
  $('#user_statuses_filter').html('<%= j render 'admin2/users/manage_users/statuses', presenter: @presenter, current: nil %>');
  $('#status_').val(current);
<% end %>

